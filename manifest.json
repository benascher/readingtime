{
    "manifest_version": 2,
    "name": "Instagram Highest Liked Post",
    "version": "1.0",
    
    "description": "A Chrome extension to navigate to the highest liked post on Instagram.",
    "permissions": [
        "tabs",
        "https://www.instagram.com/*"
    ],
    

      "browser_action": {
        "default_popup": "popup2.html",
        "default_icon": {
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
        }
      },
      "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    }
